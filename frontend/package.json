    "driver.js": "^1.3.1",
    "element-plus": "^2.7.7",
    "events": "^3.3.0",
    "fabric": "^5.3.0",
    "fontfaceobserver": "^2.3.0",
    "hotkeys-js": "^3.13.7",
    "jsbarcode": "^3.11.6",
    "lodash-es": "^4.17.21",
    "number-precision": "^1.6.0",
    "pinia": "^2.2.2",
    "qr-code-styling": "1.6.0-rc.1",
    "qs": "^6.12.3",
    "tapable": "^2.2.1",
    "uuid": "^10.0.0",
    "vue": "^3.4.33",
    "vue-i18n": "^9.13.1",
    "vue-masonry": "^0.16.0",
    "vue-router": "^4.4.0",
    "vue3-lazyload": "^0.3.8"
  },
  "devDependencies": {
    "@types/events": "^3.0.3",
    "@types/fabric": "^5.3.8",
    "@types/fontfaceobserver": "^2.1.3",
    "@types/lodash-es": "^4.17.12",
