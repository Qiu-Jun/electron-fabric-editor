    "@vitejs/plugin-vue": "^5.1.0",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "~7.0.3",
    "dotenv": "^16.4.5",
    "git-cz": "^4.9.0",
    "husky": "~9.0.11",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "unocss": "^0.58.0",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.27.3",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "~1.8.27"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@typescript-eslint",
        "postcss"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=9.0.2"
  },
  "lint-staged": {
    "packages/**/.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
